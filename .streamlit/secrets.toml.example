# Streamlit Secrets 配置示例
# 
# 使用方法：
# 1. 将此文件复制为 secrets.toml（不要提交到Git）
# 2. 将下面的示例值替换为你的实际API Key
# 3. 在Streamlit Cloud部署时，直接在Web界面的Secrets设置中添加这些值

# Etherscan API Key（必需）
# 获取地址：https://etherscan.io/apis
ETHERSCAN_API_KEY_Reconciliation = ""

# 链ID（可选，默认为1 - Ethereum主网）
# 1 = Ethereum Mainnet
# 56 = BSC
# 137 = Polygon
ETHERSCAN_CHAIN_ID = 1

